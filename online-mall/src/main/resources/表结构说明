#====用户模块======================================================
用户表:onlinemall_user
用户ID userid　  UUID varchar(64) primarykey
昵称   account varchar(64)
手机号 phonenumber varchar(64)
邮箱   mail varchar(64)
QQ号   qqnumber varchar(64)
微信   weixinnumber varchar(64)
微博   weibonumber varchar(64)
密码   password varchar(64)
姓名   name varchar(64)
性别   sex varchar(64)
生日   birthday varchar(64)
账户安全份　securitydegree int
支付密码 securitypay varchar(1) (1:启动 0:不启动)
手机验证　securityphone varchar(1) (1:启动 0:不启动)
邮箱验证 securitymail varchar(1) (1:启动 0:不启动)
真实姓名　realname varchar(64)
身份证号码　identitynumber varchar(64)
身份证照片正面 photourl1 varchar(64)
身份证照片背面 photourl2 varchar(64)
安全问题1　securityproblem1 varchar(64)
安全问题答案1　securityansower1 varchar(64)
安全问题2　securityproblem2 varchar(64)
安全问题答案2　securityansower2 varchar(64)
注册时间 registertime DATE

用户地址表onlinemall_address
记录id id UUID varchar(64) primarykey
用户id userid　  UUID varchar(64)
地址　userAddress varchar(1000)

用户的银行卡表onlinemall_bank
记录id id UUID varchar(64) primarykey
用户id userid　  UUID varchar(64)
银行名称 bankname varchar(64)
银行卡类型 banktype varchar(2) (1:信用卡 2:储蓄卡 3:借计卡)
银行卡号 banknumber varchar(64)
绑定状态 bankbindstatus varchar(2) (1:绑定 0:未绑定)

用户积分表onlinemall_user_point
记录ID UUID varchar(64)　primarykey　
用户ID userid　UUID varchar(64)
积分详情 pointinfodetail varchar(64)
积分变动 pointchange varchar(10)
积分类别 pointtype varchar(2) (1:支出 0:获得)
积分变动日期 pointtime data

用户优惠券onlinemall_user_discount
记录ID UUID varchar(64)　primarykey
用户ID userid　UUID varchar(64)
优惠券编号　discountid varhcar(64) 随机
优惠券详情　discountinfodetail varchar(64)
优惠券类别　discounttype varchar(2) (1:未过期 0:过期)

用户红包onlinemall_user_red_envelopes
记录ID UUID varchar(64)　primarykey
用户ID userid　UUID varchar(64)
红包详情 redenvelopesinfodetail varchar(64)
红包类别 redenvelopestype varchar(2) (1:未过期 0:过期)
红包剩余金额　redenvelopesmoney varchar(20)
红包有效期 redenvelopesdelinetime date
红包状态　 redenvelopesstatus　varchar(1) (1:可以用 0:不可以使用)

用户账户余额onlinemall_user_balance
记录ID UUID varchar(64)　primarykey
用户ID userid　UUID varchar(64)
余额详情 balanceinfodetail varchar(1000)
剩余金额 balancenumber int
具体的时间 balancetime date

用户的收藏onlinemall_user_collect
记录ID UUID varchar(64)　primarykey
用户ID userid　UUID varchar(64)
商品的id goodsid UUID varchar(64)
收藏时间 collectime date

用户的足迹onlinemall_user_record
记录ID UUID varchar(64)　primarykey
用户ID userid　UUID varchar(64)
商品的id goodsid UUID varchar(64)
记录时间　recordtime date
